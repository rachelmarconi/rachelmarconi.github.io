    "@vitest/ui": "^1.5.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "svelte": "^4.2.15",
    "svelte-check": "^3.6.9",
    "typescript": "^5.4.5",
    "vite": "^5.2.9",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "@urbaninstitute/dataviz-components": "^0.11.1"
  }
